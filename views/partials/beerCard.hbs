<div class='col-md-4'>
  <div class='card-group'>
    <img class='card-img-top img-responsive' src='{{this.image_url}}' alt='{{this.name}}' />
    <div class='card-body'>
      <h5 class='card-title'>{{this.name}}</h5>
      <p class='card-text'>{{this.description}}</p>
      <p class='card-text'>{{this.tagline}}</p>
    </div>
  </div>
</div>